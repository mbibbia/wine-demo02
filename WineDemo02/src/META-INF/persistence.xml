<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="pt_wine_inventory_2">  
  
  <class>ch.bibbias.persistence.objects.CountryEntity</class>
  <class>ch.bibbias.persistence.objects.RegionEntity</class>
  <class>ch.bibbias.persistence.objects.ProducerEntity</class>
  <class>ch.bibbias.persistence.objects.WineClassificationEntity</class>
  <class>ch.bibbias.persistence.objects.WineTypeEntity</class>
  <class>ch.bibbias.persistence.objects.WineEntity</class>
 
  
  
<properties>  
<property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>  
<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pt_wine_inventory_2?autoReconnect=true&amp;useSSL=false"/>  
<property name="javax.persistence.jdbc.user" value="root"/>  
<property name="javax.persistence.jdbc.password" value="adminroot"/>
<property name="eclipselink.logging.level" value="SEVERE"/>  
<property name="eclipselink.ddl-generation" value="create-or-extend-tables"/>  
</properties>  
	</persistence-unit>
</persistence>
